<h1>Listing shipping_rates</h1>

<table>
  <tr>
    <th>Shipping method</th>
    <th>Rate</th>
    <th>Shipping rate type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @shipping_rates.each do |shipping_rate| %>
  <tr>
    <td><%= shipping_rate.shipping_method_id %></td>
    <td><%= shipping_rate.rate %></td>
    <td><%= shipping_rate.shipping_rate_type_id %></td>
    <td><%= link_to 'Show', shipping_rate %></td>
    <td><%= link_to 'Edit', edit_shipping_rate_path(shipping_rate) %></td>
    <td><%= link_to 'Destroy', shipping_rate, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Shipping rate', new_shipping_rate_path %>
