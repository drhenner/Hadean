<fieldset id="admin-prototype-names">
  <div class='span-3'>
    <%= form.label :name %>
  </div>
  <div class='span-6 last'>
    <%= form.text_field :name %>
  </div>

  <div class='span-3'>
    <%= form.label :active %>
  </div>
  <div class='span-6 last'>
    <%= form.check_box :active %>
  </div>

</fieldset>
<fieldset id="admin-property-names">
  <legend>Properties</legend>
  <div id="prototype_properties">
    <%= render  :partial    => "/admin/merchandise/prototypes/property_form", 
                :locals     => { :form => form } %>
  </div>
</fieldset>