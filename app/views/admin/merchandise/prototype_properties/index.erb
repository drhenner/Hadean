<h2> Prototype Properties </h2>

<div id="admin_prototype_properties_page_wrapper">
  <div id="admin_prototype_properties_grid_wrapper">
    <%= raw jqgrid( "Prototype Properties", 
                    "admin_prototype_properties_grid", 
                    admin_merchandise_prototype_properties_path,
      [
        { :field => "display_name",           :label => "Name",     :width => 355,  :resizable => false },
        { :field => "property_display_names", :label => "Active?",  :width => 70,   :resizable => false }
      ]
    ) %>
  </div>

  <div  id="admin_new_prototype_property_button" 
        class="admin_new_button last">
    <%= link_to "New Prototype Property", new_admin_merchandise_prototype_property_path, 
                                          :class  => "button", 
                                          :id     => "new_prototype_property_button" %>
  </div>
</div>
<%= jqgrid_stylesheets %>
<%= jqgrid_javascripts %>