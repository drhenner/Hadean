    <td><%= return_authorization.amount %></td>
    <td><%= return_authorization.restocking_fee %></td>
    <td><%= return_authorization.order_id %></td>
    <td><%= return_authorization.state %></td>
    <td><%= return_authorization.created_by %></td>
    <td><%= link_to 'Show', admin_rma_return_authorization_path( return_authorization) %></td>
    <td><%= link_to 'Edit', edit_admin_rma_return_authorization_path(return_authorization) %></td>
  </tr>
<% end %>
