<h1>New purchase_order</h1>

<%= form_for :purchase_order, @purchase_order, :url  => admin_inventory_purchase_orders_path, 
                        :html => {:class => ''}  do |form| %>
  <%= render :partial => '/admin/inventory/purchase_orders/form', :locals => {:form => form}%>

  <div class="button generic_submit_button">
    <%= submit_tag "Create", 
                    :class  => "button", 
                    :id     => "create_purchase_order_button" %>
  </div>
<% end %>


<%= link_to 'Back', admin_inventory_purchase_orders_path %>